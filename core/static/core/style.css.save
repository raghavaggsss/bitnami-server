.fileUpload {
	position: relative;
	overflow: hidden;
	margin: 10px;
}
.fileUpload .upload {
	position: absolute;
	right: 0;
	margin: 0;
	padding: 0;
	font-size: 20px;
	cursor: pointer;
	opacity: 0;
	filter: alpha(opacity=0);
}

